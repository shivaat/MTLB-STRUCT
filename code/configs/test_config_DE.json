{
  "mode": "TEST",
  "data": {
    "language": "DE",
    "include_dev_examples": false,
    "data_path": "../data/1.2/",
    "max_len": 110,
    "do_lower_case": true
  },
  "model": {
    "pretrained_model_name": "bert-base-german-cased",
